{
    "compilerOptions": {
      "outDir": "./dist",
      "target": "es5",
      "module": "commonjs",
      "declaration": true,
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": [
        "es2015",
        "dom"
      ],
      "noImplicitAny": true,
      "suppressImplicitAnyIndexErrors": true
    },
    "exclude": [
      "node_modules",
      "dist",
      "examples",
      "templates"
    ]
  }