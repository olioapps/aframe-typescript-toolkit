      "server:dev": "webpack-dashboard -- webpack-dev-server --config ./webpack.config.js --hot --inline --progress --watch --open"
    },
    "license": "ISC",
    "devDependencies": {
      "awesome-typescript-loader": "^3.2.1",
      "cross-env": "^5.0.1",
      "eslint": "^4.2.0",
      "html-webpack-plugin": "^2.29.0",
      "karma": "^6.3.16",
      "rimraf": "^2.6.1",
      "source-map-loader": "^0.2.1",
      "tslint": "^5.5.0",
      "tslint-loader": "^3.5.3",
      "typescript": "^3.0.1",
      "webpack": "^3.3.0",
      "webpack-dashboard": "^0.4.0",
      "webpack-dev-server": "2.5.1"
    },
    "dependencies": {
      "@types/aframe": "^0.8.3",
      "babel-core": "^6.22.1",
      "babel-eslint": "^7.1.1",
      "babel-loader": "^6.2.10",
      "raw-loader": "^0.5.1"
    }
  }
  